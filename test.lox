for(var a = 1; a <= 100; a = a + 1) {
    var printNum = true;
    var fizzbuzz = "";
    if(a % 3 == 0) {
        printNum = false;
        fizzbuzz = fizzbuzz + "Fizz";
    }

    if(a % 5 == 0) {
        printNum = false;
        fizzbuzz = fizzbuzz + "Buzz";
    }

    if(printNum) {
        print a;
    } else {
        print fizzbuzz;
    }
}