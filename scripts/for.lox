print "0 to 10: ";
for(var i = 0; i < 10; i = i + 1) {
    print "i = " + stringify(i);
}

print "Reverse: ";
for (var i = 10; ; i = i - 1) {
    print "i = " + stringify(i);
    if (i == 0) {
        break;
    }
}

print "Even numbers: ";
for(var i = 0; i < 10; ) {
    if(i % 2 == 0) {
        print i;
    }

    i = i + 1;
} 

